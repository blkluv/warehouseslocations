#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules/vitest/node_modules:/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules:/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules/vitest/node_modules:/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules:/Users/thewizardofhahz/LinkTower/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@4.0.10_@types+debug@4.1.12_@types+node@22.13.11_@vitest+ui@4.0.10_happy-dom@20.0_bfef6df95b3f1de6a655c917d18070c6/node_modules/vitest/vitest.mjs" "$@"
fi
